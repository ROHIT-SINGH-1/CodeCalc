{
  "name": "CodeCalc",
  "displayName": "CodeCalc",
  "description": "A CODECALC VISUAL STUDIO CODE CALCULATOR",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "CodeCalc"
  ],
  
  "repository": {
    "type": "git",
    "repository": "https://github.com/ROHIT-SINGH-1/CodeCalc.git"
  },
  "main": "./dist/extension.js",
  "publisher": "ROHIT-SINGH",
  "icon": "icon/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "CodeCalc",
        "title": "CodeCalc",
        "icon": "icon/icon.png"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "CodeCalc",
          "group": "navigation",
          "when": "editorFocus"
        }
      ]
    },
    "keybindings": [
      {
        "command": "CodeCalc",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c",
        "when": "editorTextFocus"
      }
    ],
    "gallery": {
      "pricing": [
        {
          "currency": "USD",
          "amount": 0,
          "tier": "free"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
